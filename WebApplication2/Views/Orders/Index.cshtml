@model List<OrderDto>
@* //@model List<DoctorDto> *@
<h1>Order History</h1>
@* mapper.Map<DoctorDto>
    (doctors) *@
<ul class="list-group">
    @foreach (var i in Model)
    {
        <li class="list-group-item">[@i.Id]-[@i.Doctor.FirstName]- @i.CreatedAt.ToString("d") - <b>@i.CreatedAt.ToString("t")</b> made by @i.UserName</li>
    }
</ul>
